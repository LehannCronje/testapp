            <div class="middle-graph graph-right col-4 graph">
                <h2>Middle right</h2>
                <hr>
                <canvas id="chart-area"></canvas>
                <button id="randomizeData3">Randomize Data</button>
                <button id="addDataset3">Add Dataset</button>
                <button id="removeDataset3">Remove Dataset</button>
                
            </div>
            <div class="bottom-graph graph-right col-6 graph">
                <h2>bottom graph left</h2>
                <hr>
                <canvas id="chart-1"></canvas>
                <button onclick="randomize(this)">Randomize</button>
                <button onclick="addDataset(this)">Add Dataset</button>
                <button onclick="removeDataset(this)">Remove Dataset</button>
            </div>
            <div class="bottom-graph graph-left col-6 graph">
                <h2>bottom graph right</h2>
                <hr>
                <canvas id="chart-0"></canvas>
                <button onclick="randomize(this)">Randomize</button>
